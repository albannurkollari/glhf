  "private": true,
  "scripts": {
    "build:dev": "rm -rf build && webpack --mode development --config configs/webpack/server && webpack --mode development --config configs/webpack/web",
    "build:prod": "rm -rf build && webpack --mode production --config configs/webpack/server && webpack --mode production --config configs/webpack/web",
    "start": "webpack-dev-server --mode development --config configs/webpack/web",
    "start:debug": "webpack-dev-server --mode development --config configs/webpack",
    "server": "nodemon .src/server",
    "server:debug": "nodemon --inspect ./src/server",
    "server:start": "webpack-dev-server --mode development --config configs/webpack/server",
    "webpack:debug": "node --inspect ./node_modules/webpack/bin/webpack.js"
  },
  "devDependencies": {
